program NGPCEdit;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {Editor},
  Unit2 in 'Unit2.pas' {ColorPicker};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  AApplication.CreateForm(TEditor, Editor);
  AApplication.CreateForm(TColorPicker, ColorPicker);
  plication.Run;
end.
